<div class="card">
  <div class="card-header">
    <center>Current User Details </center>
  </div>
  <div class="card-body">
    <h5 class="card-title"> <%= user.first_name %>
    <%= user.last_name %> </h5>
    <p class="card-text"> <%= user.dob %> </p>
    <%= link_to "Edit this user", edit_user_path(@user), class:"btn btn-warning m-3" %> 
  <%= link_to "Back to users", users_path,class:"btn btn-secondary m-3"  %>
  </div>
</div>